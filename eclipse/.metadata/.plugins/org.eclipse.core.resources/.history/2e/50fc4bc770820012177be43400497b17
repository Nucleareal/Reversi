package Model.Character;

import javax.swing.ImageIcon;

import Model.ImageControler;
import Model.AI.*;

public class Character_03Yuugi implements ICharacter
{
	@Override
	public String getName()
	{
		return "ヤマメ";
	}

	@Override
	public ImageIcon getImage()
	{
		return ImageControler.CharaYamame;
	}

	@Override
	public AI getAI()
	{
		return new AI_Level_0();
	}
}

package Model.AI;

import Model.Stone;

public abstract class AI_Base implements AI
{
	private Stone _turn;

	public AI_Base(Stone turn)
	{
		_turn = turn;
	}

	public AI_Base()
	{
		this(Stone.None);
	}

	public void setStone(Stone stone)
	{
		_turn = stone;
	}

	@Override
	public Stone getTurn()
	{
		return _turn;
	}
}

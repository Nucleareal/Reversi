package Model.AI;

import Model.Position;

public class Node
{
	private Position _pos;
	private int _value;

	public Node(Position pos, int value)
	{
		_pos = pos;
		_value = value;
	}

	public int getValue()
	{
		return _value;
	}

	public boolean isBiggerThan(Node node)
	{
		if(_value > node.getValue())
			return true;
		return false;
	}
}

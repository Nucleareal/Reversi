package Model.Character;

import javax.swing.ImageIcon;

import Model.ImageControler;
import Model.AI.AI;
import Model.AI.AI_Level_0;

public class CharacterKisume implements ICharacter
{
	@Override
	public String getName()
	{
		return "キスメ";
	}

	@Override
	public ImageIcon getImage()
	{
		return ImageControler.CharaKisume;
	}

	@Override
	public AI getAI()
	{
		return new AI_Level_0A();
	}
}

package Model.AI;

import Model.Position;

public class Node
{
	private Position _pos;
	private int _value = 0;

	public Node(Position pos, int value)
	{
		_pos = pos;
		_value = value;
	}

	public int getValue()
	{
		return _value;
	}

	public Position getPosition()
	{
		return _pos;
	}

	public boolean isBiggerThan(Node node)
	{
		return _value > node.getValue();
	}
}

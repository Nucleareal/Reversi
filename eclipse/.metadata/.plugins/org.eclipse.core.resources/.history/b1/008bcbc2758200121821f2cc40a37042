		return "星熊勇儀";
	}

	@Override
	public ImageIcon[] getImage()
	{
		return ImageControler.CharaYuugi;
	}

	@Override
	public AI getAI()
	{
		return new AI_Level_2();
	}

	@Override
	public String[] getWords(CharacterState state)
	{
		return null;
	}
}

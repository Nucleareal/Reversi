package Model.Character;

import javax.swing.ImageIcon;

import Model.ImageControler;
import Model.AI.*;

public class Character_03Parsee implements ICharacter
{
	@Override
	public String getName()
	{
		return "水橋パルスィ";
	}

	@Override
	public ImageIcon getImage()
	{
		return ImageControler.CharaParsee;
	}

	@Override
	public AI getAI()
	{
		return new AI_Level_1();
	}
}
